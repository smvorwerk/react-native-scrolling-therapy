{
  "name": "ScrollingTherapy",
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "precommit": "cd scripts && sh hideEnvVars.sh",
    "copy-env": "cd scripts && sh copyEnvVars.sh ../.env && cd ..",
    "start:dev": "cat .env.development > .env && yarn copy-env && ENVFILE=.env && react-native start --reset-cache",
    "start:stg": "cat .env.staging > .env && yarn copy-env && ENVFILE=.env && react-native start --reset-cache",
    "start:prod": "cat .env.production > .env && yarn copy-env && ENVFILE=.env && react-native start --reset-cache",
    "test": "jest",
    "test:report": "jest --coverage && open ./coverage/lcov-report/index.html",
    "prepare": "cd .. && husky install && cd ScrollingTherapy",
    "bump:minor": "npx react-native bump-version --type minor --skip-code-for all",
    "bump:patch": "npx react-native bump-version --type patch --skip-code-for all",
    "codepush:development:android": "appcenter codepush release-react -a DWS-Brazil-Lab/The-Scrolling-Therapy-Android -d Development",
    "codepush:development:ios": "appcenter codepush release-react -a DWS-Brazil-Lab/The-Scrolling-Therapy-iOS -d Development",
    "codepush:development": "yarn codepush:development:android && yarn codepush:development:ios",
    "codepush:staging:android": "appcenter codepush release-react -a DWS-Brazil-Lab/The-Scrolling-Therapy-Android -d Staging",
    "codepush:staging:ios": "appcenter codepush release-react -a DWS-Brazil-Lab/The-Scrolling-Therapy-iOS -d Staging",
    "codepush:staging": "yarn codepush:staging:android && yarn codepush:staging:ios",
    "codepush:production:android": "appcenter codepush release-react -a DWS-Brazil-Lab/The-Scrolling-Therapy-Android -d Production",
    "codepush:production:ios": "appcenter codepush release-react -a DWS-Brazil-Lab/The-Scrolling-Therapy-iOS -d Production",
    "codepush:production": "yarn codepush:production:android && yarn codepush:production:ios",
    "rollback:development:android": "appcenter codepush rollback -a DWS-Brazil-Lab/The-Scrolling-Therapy-Android Development",
    "rollback:development:ios": "appcenter codepush rollback -a DWS-Brazil-Lab/The-Scrolling-Therapy-iOS Development",
    "rollback:development": "yarn rollback:development:android && yarn rollback:development:ios",
    "rollback:stagin:android": "appcenter codepush rollback -a DWS-Brazil-Lab/The-Scrolling-Therapy-Android Staging",
    "rollback:staging:ios": "appcenter codepush rollback -a DWS-Brazil-Lab/The-Scrolling-Therapy-iOS Staging",
    "rollback:staging": "yarn rollback:staging:android && yarn rollback:staging:ios",
    "rollback:production:android": "appcenter codepush rollback -a DWS-Brazil-Lab/The-Scrolling-Therapy-Android Production",
    "rollback:production:ios": "appcenter codepush rollback -a DWS-Brazil-Lab/The-Scrolling-Therapy-iOS Production",
    "rollback:production": "yarn rollback:production:android && yarn rollback:production:ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-firebase/analytics": "^17.4.2",
    "@react-native-firebase/app": "^17.3.2",
    "@react-native-firebase/auth": "^17.3.2",
    "@react-native-firebase/crashlytics": "^17.4.2",
    "@react-native-firebase/firestore": "^17.3.2",
    "@react-navigation/drawer": "^6.6.0",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "date-fns": "^2.29.3",
    "face-api.js": "^0.22.2",
    "i18next": "^22.4.13",
    "lottie-ios": "3.4.1",
    "lottie-react-native": "^5.1.5",
    "metro-hermes-compiler": "^0.74.0",
    "react": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-native": "0.71.0",
    "react-native-code-push": "^8.0.1",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-reanimated": "^3.0.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.8.0",
    "react-native-video": "^5.2.1",
    "react-native-webview": "^11.26.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro": "^0.74.0",
    "metro-react-native-babel-preset": "0.73.5",
    "prettier": "^2.4.1",
    "react-native-cli-bump-version": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-navigation)"
    ]
  }
}